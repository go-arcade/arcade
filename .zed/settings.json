{
  "lsp": {
    "protobuf": {
      "command": "protobuf-language-server",
      "args": [
        "--stdio"
      ],
      "initialization_options": {
        "protocPath": "/usr/local/bin/protoc",
        "includePaths": [
          "api",
          "third_party",
          "/usr/local/include"
        ]
      }
    }
  }
}
